<div class="page-header">
  <%= link_to abstract_docs_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_abstract_doc_path(@abstract_doc), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show Abstract</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @abstract_doc.title %></dd>

  <dt>Attachment:</dt>
  <dd><%= @abstract_doc.attachment %></dd>

  <dt>Category:</dt>
  <dd><%= @abstract_doc.category %></dd>

  <dt>User:</dt>
  <dd><%= @abstract_doc.user %></dd>

</dl>
