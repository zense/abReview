<div class="page-header">
  <%= link_to reviews_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_review_path(@review), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show review</h1>
</div>

<dl class="dl-horizontal">
  <dt>User:</dt>
  <dd><%= @review.user %></dd>

  <dt>Abstract doc:</dt>
  <dd><%= @review.abstract_doc %></dd>

  <dt>Originality:</dt>
  <dd><%= @review.originality %></dd>

  <dt>Expand:</dt>
  <dd><%= @review.expand %></dd>

  <dt>Impact:</dt>
  <dd><%= @review.impact %></dd>

  <dt>Methods:</dt>
  <dd><%= @review.methods %></dd>

  <dt>Clarity:</dt>
  <dd><%= @review.clarity %></dd>

  <dt>Comments:</dt>
  <dd><%= @review.comments %></dd>

  <dt>Attachment:</dt>
  <dd><%= link_to @review.attachment_url, @review.attachment_url %></dd>


</dl>
