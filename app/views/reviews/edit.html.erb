<div class="page-header">
  <%= link_to reviews_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to @review, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-info-sign"></span>
    Show
  <% end %>
  <h1>Review for <%= @review.abstract_doc.title %></h1>
</div>

<%= render 'form' %>
